{
    "name": "instapay/appi",
    "type": "project",
    "description": "Siamcardeal services API",
    "keywords": ["instapay", "api"],
    "license": "MIT",
    "require": {
        "php": "8.1.17",
        "doctrine/dbal": "3.5.1",
        "guzzlehttp/guzzle": "7.5.0",
        "kalnoy/nestedset": "6.0.1",
        "laravel/framework": "9.38.0",
        "laravel/passport": "10.4.1",
        "laravel/sanctum": "2.15.1",
        "laravel/tinker": "2.7.2",
        "league/flysystem-aws-s3-v3": "3.10.0",
        "maatwebsite/excel": "3.1.48",
        "mll-lab/laravel-graphql-playground": "2.6.0",
        "nanigans/single-table-inheritance": "1.0.0",
        "nuwave/lighthouse": "5.64.1",
        "rap2hpoutre/fast-excel": "5.2.0"
    },
    "require-dev": {
        "fakerphp/faker": "1.20.0",
        "friendsofphp/php-cs-fixer": "3.13.0",
        "kevincobain2000/laravel-erd": "1.6",
        "laravel/sail": "1.16.2",
        "mockery/mockery": "1.5.1",
        "nunomaduro/collision": "6.3.1",
        "phpunit/phpunit": "9.5.26",
        "spatie/laravel-ignition": "1.6.1"
    },
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Api\\": "api/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
